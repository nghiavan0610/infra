# =============================================================================
# HashiCorp Vault Configuration
# =============================================================================

# Vault port
VAULT_PORT=8200

# Vault address (for CLI)
VAULT_ADDR=http://127.0.0.1:8200

# UI enabled
VAULT_UI=true

# Log level
VAULT_LOG_LEVEL=info

# Resource limits
MEMORY_LIMIT=512m
CPU_LIMIT=1
